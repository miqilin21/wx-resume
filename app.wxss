page {
    color: #333;
}

.container {
    box-sizing: border-box;
    background-color: #f2f2f2;
    font-family: PingFangSC-Light,helvetica,'Heiti SC';
    margin-bottom: 40rpx;
}

view,image,text,navigator {
    box-sizing: border-box;
    padding: 0;
    margin: 0;
}

image {
    width: 100%;
}

view {
    font-family: PingFangSC-Light,helvetica,'Heiti SC';
    font-size: 30rpx;
}

.strong {
    font-weight: 700;
}

.row {
    margin-bottom: 20rpx;
}

.gray {
    color: #a1a1a1;
}

.main {
    width: 100%;
    padding: 0 20rpx 20rpx;
}

.section {
    background-color: #fff;
    border-radius: 10rpx;
    padding: 30rpx;
    margin-bottom: 30rpx;
}

.section .title {
    border-bottom: 1px solid #f2f2f2;
    padding-bottom: 10rpx;
    margin-bottom: 20rpx;
}

.section .title text {
    border-left: 6rpx solid #07c160;
    padding-left: 20rpx;
}

.section .content {
    margin-left: 20rpx;
}

.banner image {
    width: 100%;
}

.flex-between {
    display: flex;
    justify-content: space-between;
}

.banner-tmp {
    position: relative;
}

.banner-tmp .banner-text {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    padding: 30rpx;
    width: 70%;
    background-color: rgba(255,255,255,0.8);
    border-radius: 10rpx;
    text-align: center;
}

.project-tmp {
    padding: 40rpx;
}

.project-title {
    margin: 60rpx 40rpx;
    font-size: 40rpx;
    text-align: center;
    font-weight: 700;
}

.project-label {
    text-align: center;
    margin-bottom: 60rpx;
}

.project-label text {
    font-size: 12px;
    padding: 6rpx 15rpx;
    border: 1px solid #f2f2f2;
    border-radius: 10rpx;
    margin-right: 15rpx;
    color: #999;
    display: inline-block;
}

.project-info .label {
    margin-bottom: 10rpx;
}

.project-info .label::before {
    content: '';
    display: inline-block;
    vertical-align: middle;
    width: 12rpx;
    height: 12rpx;
    margin-right: 15rpx;
    border-radius: 50%;
    background-color: #79c4c5;
    margin-top: -4px;
}

.project-info .desc {
    padding-left: 28rpx;
    color: #666;
}

.project-info .item {
    border-bottom: 1px solid #f2f2f2;
    padding-bottom: 20rpx;
    margin-bottom: 40rpx;
}

.project-info .item:last-child {
    border-bottom: 0;
}